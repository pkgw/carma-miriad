!		Miriad Mxv.ad file
!       For MXV V2.0 [04aug92]
!
*MXVform.foreground:         yellow
*MXVform.background:         navyblue
*MXVlabel.foreground:        yellow
*MXVlabel.background:        navyblue
*MXVtoggle.foreground:       navyblue
*MXVtoggle.background:       lightblue
*Quit.foreground:            red
*Quit.background:            green
*MXVbutton.foreground:       navyblue
*MXVbutton.background:       yellow
*MXVbox.borderColor:         red
*MXVbulletin.borderColor:    green
!
*font:                       6x13
!
*MXV_Control.rows	     20
*MXV_Control.Columns	     80
*List.Columns	55
*List.Rows:     16
!
